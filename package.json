{
  "name": "collectio",
  "version": "0.0.0",
  "description": "Manage all your machine learning datasets in one place",
  "main": "src/index.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=production node build/index.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --ignore-watch ['src'] src/index.ts",
    "build": "webpack --mode=production --config src/webpack.config.ts",
    "tsc": "tsc && tsc-alias",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "test:server": "jest --verbose --runInBand --rootDir=src/server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/errorfourten/collectio.git"
  },
  "keywords": [
    "dataset",
    "machine",
    "learning"
  ],
  "author": "",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/errorfourten/collectio/issues"
  },
  "homepage": "https://github.com/errorfourten/collectio#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "chokidar": "^3.5.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "fomantic-ui-css": "^2.8.7",
    "formik": "^2.2.9",
    "module-alias": "^2.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.16.0",
    "react-router-dom": "^5.2.0",
    "semantic-ui-react": "^2.0.3",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "babel-loader": "^8.2.2",
    "core-js": "^3.13.1",
    "css-loader": "^5.2.6",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.0.4",
    "regenerator-runtime": "^0.13.7",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.2",
    "ts-node-dev": "^1.1.6",
    "tsc-alias": "^1.3.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-middleware": "^4.3.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleNameMapper": {
      "^@controllers/(.*)$": "<rootDir>/controllers/$1",
      "^@middleware/(.*)$": "<rootDir>/middleware/$1",
      "^@util/(.*)$": "<rootDir>/util/$1",
      "^@services/(.*)$": "<rootDir>/services/$1"
    }
  },
  "_moduleAliases": {
    "@root": "./src",
    "@controllers": "src/server/controllers",
    "@middleware": "src/server/middleware",
    "@util": "src/server/util"
  }
}
